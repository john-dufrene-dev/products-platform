{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "darkaonline/l5-swagger": "^6.0",
        "doctrine/dbal": "~2.3",
        "dompdf/dompdf": "^2.0.0",
        "fideloper/proxy": "^4.2",
        "h4cc/wkhtmltoimage-i386": "0.12.x",
        "h4cc/wkhtmltopdf-i386": "0.12.x",
        "infyomlabs/adminlte-templates": "6.0.x-dev",
        "infyomlabs/coreui-templates": "6.0.x-dev",
        "infyomlabs/generator-builder": "dev-master",
        "infyomlabs/laravel-generator": "6.0.x-dev",
        "infyomlabs/routes-explorer": "dev-master",
        "infyomlabs/swagger-generator": "dev-master",
        "intervention/image": "^2.5",
        "intervention/imagecache": "^2.4",
        "jeroennoten/laravel-adminlte": "^3.0",
        "laravel/framework": "^6.0",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "maatwebsite/excel": "^3.1",
        "renatomarinho/laravel-page-speed": "^1.8",
        "spatie/laravel-permission": "^3.6",
        "tymon/jwt-auth": "^1.0",
        "yajra/laravel-datatables-buttons": "^4.6",
        "yajra/laravel-datatables-html": "^4.4",
        "yajra/laravel-datatables-oracle": "~9.0",
        "zircote/swagger-php": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
